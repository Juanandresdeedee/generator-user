const items = ['book', 'pen', 'pencil', 'color'];


for (const item of items){

        console.log(item);



}




const items1 = ['book', 'pen', 'pencil', 'color'];


for (let i=0; i<items1.length; i++){

        console.log(items1[i]);



}


const users = [{name: 'John'}, {name: 'Smith'},{name: 'juan'},{name: 'pedro'}];

for (const user of users){

        console.log (user.name);
}


const str = 'hello world';
for ( const letter of str){
        console.log(letter);

}

// LOOP OVER MAP


const map = new Map();
map.set('name', 'John');
map.set('age' , 30);

for (const [key,value] of map){

        console.log(key,value);


}



const post =  {
 
id: 1,
title: 'My First Post',
body : 'This is body',

};

// convert json string 

const str = JSON.stringify(post);


// parse json string

const obj = JSON.parse(str);    

// console.log(str);


console.log(obj);


// can be an array too json


const posts = [
    {
 
        id: 1,
        title: 'My First Post',
        body : 'This is body',
        
        },

        {
 
            id: 2,
            title: 'Post two',
            body : 'This is body two',
            
 },  


];

const str2 = JSON.stringify(posts);  
const obj2 = JSON.parse(str2);    


console.log(obj2);

console.log(str2);



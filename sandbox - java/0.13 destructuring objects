


const firtsName = 'John';
const lastName = 'Doe';
const age = 35;


const person = {

        firtsName,
        lastName,
        age,
}

console.log(person.age);



//destructuring


const todo = {

    id: 1,
    title: 'Todo One',
    user:{
        name: 'John',
        age: 35,
        nationality: 'American',


    },
};



const {
     id:todoId,
     title,
     user: {name}
    }= todo;


console.log(todoId);



//destructure arrays 

const numbers = [ 1, 2, 3, 4, 5, 6, 7, 8];

const[firts, second,...rest ]= numbers
// const[uno,dos]= numbers


// console.log(uno, dos);





console.log(firts, second,rest);

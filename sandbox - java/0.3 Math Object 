x = Math.sqrt(9);


x = Math.abs(-5);


x = Math.round(9.8);


x = Math.ceil(5.2);

x = Math.floor(5.2);

x = Math.pow(3,4);

x = Math.min(6,3,4,5,6,7,);

x = Math.max(6,3,4,5,6,7,);


x = Math.random();

x = Math.random()*18;
// from 1 to 10
x = Math.random()*10+1;

x = Math.floor(Math.random()*10+1);

x = Math.floor(Math.random()*100+1);


